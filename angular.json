{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@addapptables/ng-web-socket": {
      "root": "projects/addapptables/ng-web-socket",
      "sourceRoot": "projects/addapptables/ng-web-socket/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/addapptables/ng-web-socket/tsconfig.lib.json",
            "project": "projects/addapptables/ng-web-socket/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/addapptables/ng-web-socket/src/test.ts",
            "tsConfig": "projects/addapptables/ng-web-socket/tsconfig.spec.json",
            "karmaConfig": "projects/addapptables/ng-web-socket/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/addapptables/ng-web-socket/tsconfig.lib.json",
              "projects/addapptables/ng-web-socket/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
